{
  "name": "@zeroprompt/api",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "TS_NODE_TRANSPILE_ONLY=1 ts-node-dev --respawn --ignore-watch node_modules --poll src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "sync:models": "TS_NODE_TRANSPILE_ONLY=1 ts-node-dev --respawn src/scripts/syncModels.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.937.0",
    "@aws-sdk/s3-request-presigner": "^3.937.0",
    "@prisma/client": "^5.16.2",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.25",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ethers": "^6.15.0",
    "express": "^4.19.2",
    "gpt-tokenizer": "^3.4.0",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^10.0.0",
    "prettier": "^3.2.5",
    "prisma": "^5.16.2",
    "supertest": "^6.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  }
}
